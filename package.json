{
  "name": "rntextaudiointerleave",
  "main": "expo-router/entry",
  "version": "1.0.0",
  "scripts": {
    "start": "expo start --dev-client",
    "start:expo-go": "expo start",
    "android": "expo start --android --dev-client",
    "ios": "expo start --ios --dev-client",
    "web": "expo start --web",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "prepare": "husky",
    "build:web": "expo export --platform web",
    "build:android": "expo run:android --variant release",
    "build:ios": "expo run:ios --configuration Release",
    "build:dev-android": "eas build --platform android --profile development",
    "build:dev-ios": "eas build --platform ios --profile development",
    "build:preview-android": "eas build --platform android --profile preview",
    "build:preview-ios": "eas build --platform ios --profile preview",
    "build:dev-all": "eas build --platform all --profile development",
    "build:preview-all": "eas build --platform all --profile preview",
    "prebuild": "expo prebuild",
    "prebuild:clean": "rm -rf android ios && expo prebuild",
    "prebuild:android": "expo prebuild --platform android",
    "prebuild:ios": "expo prebuild --platform ios",
    "rn:start": "react-native start",
    "rn:android": "react-native run-android",
    "rn:ios": "react-native run-ios",
    "rn:android:debug": "react-native run-android --variant debug",
    "rn:android:release": "react-native run-android --variant release",
    "rn:ios:debug": "react-native run-ios --configuration Debug",
    "rn:ios:release": "react-native run-ios --configuration Release",
    "rn:clean": "cd android && ./gradlew clean && cd ..",
    "rn:install": "cd android && ./gradlew installDebug && cd ..",
    "rn:install:release": "cd android && ./gradlew installRelease && cd ..",
    "rn:setup": "npm run prebuild:android && npm run rn:install"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,json,md}": [
      "prettier --write"
    ]
  },
  "dependencies": {
    "@expo/vector-icons": "^14.1.0",
    "@react-navigation/bottom-tabs": "^7.3.10",
    "@react-navigation/elements": "^2.3.8",
    "@react-navigation/native": "^7.1.6",
    "expo": "~53.0.17",
    "expo-audio": "~0.4.8",
    "expo-constants": "~17.1.7",
    "expo-font": "~13.3.2",
    "expo-image": "~2.3.2",
    "expo-linking": "~7.1.7",
    "expo-router": "~5.1.3",
    "expo-splash-screen": "~0.30.10",
    "expo-status-bar": "~2.2.3",
    "expo-system-ui": "~5.0.10",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "react-native": "0.79.5",
    "react-native-gesture-handler": "~2.24.0",
    "react-native-reanimated": "~3.17.4",
    "react-native-safe-area-context": "5.4.0",
    "react-native-screens": "~4.11.1",
    "react-native-web": "~0.20.0",
    "react-native-webview": "13.13.5"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@react-native-community/cli": "^13.6.0",
    "@testing-library/jest-native": "^5.4.3",
    "@testing-library/react-native": "^12.4.3",
    "@trivago/prettier-plugin-sort-imports": "^5.2.2",
    "@types/jest": "^29.5.8",
    "@types/react": "~19.0.10",
    "expo-dev-client": "^5.2.4",
    "husky": "^9.1.7",
    "jest": "^29.7.0",
    "lint-staged": "^16.1.2",
    "prettier": "^3.2.5",
    "react-test-renderer": "19.0.0",
    "typescript": "~5.8.3"
  },
  "private": true
}
